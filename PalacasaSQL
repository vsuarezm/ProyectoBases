-- -----------------------------------------------------
-- Table `mydb`.`cambio_precios`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `palacasa`.`cambio_precios` (
  `idCambio` INT auto_increment NOT NULL,
  `idProducto` INT NOT NULL,
  `precioNuevo` INT NOT NULL,
  `precioViejo` INT NOT NULL,
  `fecha` DATE NOT NULL,
  `usuario` VARCHAR(30) NOT NULL,
PRIMARY KEY (`idCambio`),
  CONSTRAINT `idProducto_Cambio`
    FOREIGN KEY (`idProducto`)
    REFERENCES `palacasa`.`producto` (`idProducto`))
ENGINE = InnoDB;

