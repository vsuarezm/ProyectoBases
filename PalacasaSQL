-- -----------------------------------------------------
-- Table `mydb`.`Administradores`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `palacasa`.`Administradores` (
  `usuario` VARCHAR(30) NOT NULL,
  `email` VARCHAR(80) NOT NULL,
  `contrasena` VARCHAR(10) NOT NULL,
  `nombre` VARCHAR(30) NOT NULL,
  PRIMARY KEY (`usuario`))
ENGINE = InnoDB;
